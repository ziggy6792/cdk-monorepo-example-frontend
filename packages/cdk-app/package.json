{
  "name": "@danielblignaut/cdk-app",
  "version": "0.1.0",
  "bin": {
    "packages": "src/index.ts"
  },
  "scripts": {
    "build": "tsc -p tsconfig.build.json",
    "watch": "tsc -w -p tsconfig.build.json",
    "synth": "npx cdk synth",
    "test": "jest",
    "cdk": "cdk",
    "clean:light": "rm -rf ./dist && rm -rf tsconfig.build.tsbuildinfo",
    "clean:full": "yarn run clean:light && rm -rf ./node_modules && rm -rf ./cdk.out"
  },
  "dependencies": {
    "@aws-cdk/aws-codepipeline": "1.77.0",
    "@aws-cdk/aws-codepipeline-actions": "1.77.0",
    "@aws-cdk/aws-s3": "1.77.0",
    "@aws-cdk/aws-s3-deployment": "1.77.0",
    "@aws-cdk/aws-ssm": "1.77.0",
    "@aws-cdk/core": "1.77.0",
    "@aws-cdk/pipelines": "1.77.0",
    "@aws-solutions-constructs/core": "1.77.0",
    "@danielblignaut/web-app": "^0.1.0",
    "aws-cdk": "1.77.0",
    "cdk-spa-deploy": "1.77.0"
  },
  "devDependencies": {
    "tsconfig-paths": "^3.9.0"
  }
}
