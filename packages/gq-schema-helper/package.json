{
    "name": "@alpaca-frontend/gq-schema-helper",
    "bin": {
        "gq-schema-helper": "./dist/index.js"
    },
    "version": "1.0.0",
    "scripts": {
        "test": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/index.ts",
        "start": "tsc -w",
        "build": "tsc -p .",
        "local": "sudo npm i -g && gq-schema-helper",
        "typecheck": "tsc --noEmit",
        "precommit": "yarn run typecheck"
    },
    "dependencies": {
        "@types/node": "^14.14.21",
        "axios": "^0.21.1",
        "json-beautify": "^1.1.1",
        "nodemon": "^2.0.7",
        "ts-node": "^9.1.1",
        "typescript": "^4.1.3",
        "yargs": "^16.2.0"
    }
}
