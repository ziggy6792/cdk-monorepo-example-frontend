{
    "name": "@alpaca-frontend/web-app",
    "version": "0.1.0",
    "private": true,
    "main": "builds/index.js",
    "dependencies": {
        "@apollo/client": "^3.3.12",
        "@apollo/react-hooks": "^4.0.0",
        "@aws-amplify/auth": "^3.4.15",
        "@date-io/date-fns": "1.3.13",
        "@graphql-codegen/cli": "1.9.1",
        "@lifeomic/axios-fetch": "^1.4.2",
        "@material-ui/core": "^4.11.3",
        "@material-ui/icons": "^4.11.2",
        "@material-ui/lab": "^4.0.0-alpha.57",
        "@material-ui/pickers": "^3.3.10",
        "@reduxjs/toolkit": "^1.5.0",
        "@testing-library/react": "^11.1.0",
        "@testing-library/user-event": "^12.1.10",
        "@types/jest": "^26.0.20",
        "@types/react-beautiful-dnd": "^13.0.0",
        "ace-builds": "^1.4.12",
        "aws-amplify": "^3.3.12",
        "aws4-axios": "^2.0.1",
        "axios": "^0.19.2",
        "date-fns": "^2.19.0",
        "env-cmd": "^10.1.0",
        "eslint-config-airbnb-typescript": "^12.0.0",
        "eslint-loader": "^4.0.2",
        "eslint-plugin-jest": "^24.1.3",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-react": "^7.22.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "formik": "^2.2.6",
        "formik-material-ui": "^3.0.1",
        "formik-material-ui-lab": "^0.0.8",
        "formik-material-ui-pickers": "^0.0.12",
        "graphql-tag": "^2.11.0",
        "history": "^5.0.0",
        "hoist-non-react-statics": "^3.3.2",
        "immer": "^8.0.0",
        "js-logger": "^1.6.1",
        "js-yaml": "^4.0.0",
        "lodash": "^4.17.20",
        "mui-datatables": "^3.7.6",
        "react": "^17.0.1",
        "react-ace": "^9.3.0",
        "react-beautiful-dnd": "^13.1.0",
        "react-dom": "^17.0.1",
        "react-icons": "^4.1.0",
        "react-redux": "^7.2.2",
        "react-router": "^5.2.0",
        "react-router-dom": "^5.2.0",
        "react-scripts": "4.0.1",
        "typescript": "^4.0.3",
        "url": "^0.11.0",
        "use-immer": "^0.4.2",
        "web-vitals": "^0.2.4",
        "yaml": "^1.10.2",
        "yup": "^0.32.8"
    },
    "scripts": {
        "start": "yarn fetch:gq:schema --useLocal=true && yarn run-p generate start:react",
        "start:react": "react-scripts start",
        "generate": "graphql-codegen --watch",
        "build": "react-scripts --max_old_space_size=8192 build",
        "fetch:gq:schema": "yarn gq-schema-helper fetch-schema --toFile=src/graphql/fragment-types.json",
        "builds:create": "yarn builds:add:staging && yarn builds:add:prod",
        "builds:add:staging": "rm -rfv builds/staging && mkdir -p builds/staging && cp -a build builds/staging && yarn builds:config:staging",
        "builds:add:prod": "rm -rfv builds/prod && mkdir -p builds/prod && cp -a build builds/prod && yarn builds:config:prod",
        "builds:config:prod": "yarn aws-config-helper copy-config --fromSsm /alpaca-backend/prod/frontend-config --toFile builds/prod/build/config/env.json",
        "builds:config:staging": "yarn aws-config-helper copy-config --fromSsm /alpaca-backend/staging/frontend-config --toFile builds/staging/build/config/env.json",
        "builds:clean": "rm -rfv builds/prod && rm -rfv builds/staging",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "format": "prettier --write src/**/*.ts{,x}",
        "lint": "tsc --noEmit && eslint src/**/*.ts{,x}",
        "typecheck": "tsc --noEmit",
        "precommit": "yarn run typecheck"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@alpaca-frontend/aws-config-helper": "^2.0.6",
        "@alpaca-frontend/gq-schema-helper": "^1.0.0",
        "@graphql-codegen/introspection": "1.9.1",
        "@graphql-codegen/typescript": "1.9.1",
        "@graphql-codegen/typescript-operations": "1.9.1",
        "@graphql-codegen/typescript-react-apollo": "1.9.1",
        "@types/lodash": "^4.14.166",
        "@types/mui-datatables": "^3.7.1",
        "@types/node": "^14.14.16",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "@types/react-redux": "^7.1.14",
        "@types/react-router-dom": "^5.1.6",
        "change-case": "^4.1.2",
        "prettier": "^2.2.1",
        "react-app-rewired": "^2.1.8"
    }
}
