{
    "name": "@danielblignaut/web-app",
    "version": "0.1.0",
    "private": true,
    "main": "builds/index.js",
    "dependencies": {
        "@aws-amplify/auth": "^3.4.15",
        "@lifeomic/axios-fetch": "^1.4.2",
        "@material-ui/core": "^4.11.2",
        "@material-ui/icons": "^4.11.2",
        "@material-ui/lab": "^4.0.0-alpha.57",
        "@reduxjs/toolkit": "^1.5.0",
        "@testing-library/react": "^11.1.0",
        "@testing-library/user-event": "^12.1.10",
        "@types/jest": "^26.0.20",
        "apollo-cache-inmemory": "^1.6.6",
        "apollo-client": "^2.6.10",
        "apollo-link-http": "^1.5.17",
        "aws-amplify": "^3.3.12",
        "aws4-axios": "^2.0.1",
        "axios": "^0.19.2",
        "env-cmd": "^10.1.0",
        "eslint-config-airbnb-typescript": "^12.0.0",
        "eslint-loader": "^4.0.2",
        "eslint-plugin-jest": "^24.1.3",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-react": "^7.22.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "formik": "^2.2.6",
        "formik-material-ui": "^3.0.1",
        "graphql-tag": "^2.11.0",
        "history": "^5.0.0",
        "hoist-non-react-statics": "^3.3.2",
        "immer": "^8.0.0",
        "js-logger": "^1.6.1",
        "lodash": "^4.17.20",
        "react": "^17.0.1",
        "react-apollo": "^3.1.5",
        "react-dom": "^17.0.1",
        "react-icons": "^4.1.0",
        "react-redux": "^7.2.2",
        "react-router-dom": "^5.2.0",
        "react-scripts": "4.0.1",
        "typescript": "^4.0.3",
        "url": "^0.11.0",
        "use-immer": "^0.4.2",
        "web-vitals": "^0.2.4",
        "yup": "^0.32.8"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "builds:create": "yarn builds:add:staging && yarn builds:add:prod",
        "builds:add:staging": "rm -rfv builds/staging && mkdir -p builds/staging && cp -a build builds/staging && yarn builds:config:staging",
        "builds:add:prod": "rm -rfv builds/prod && mkdir -p builds/prod && cp -a build builds/prod && yarn builds:config:prod",
        "builds:config:prod": "echo $(pwd) && yarn fetch-env-config --fromSsm /cdk-monorepo-backend/prod/frontend-config --toFile builds/prod/build/config/env.js",
        "builds:config:staging": "echo $(pwd) && yarn fetch-env-config --fromSsm /cdk-monorepo-backend/staging/frontend-config --toFile builds/staging/build/config/env.js",
        "builds:clean": "rm -rfv builds/prod && rm -rfv builds/staging",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "format": "prettier --write src/**/*.ts{,x}",
        "lint": "tsc --noEmit && eslint src/**/*.ts{,x}",
        "typecheck": "tsc --noEmit",
        "precommit": "yarn run typecheck"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@danielblignaut/aws-env-config": "^2.0.6",
        "@types/lodash": "^4.14.166",
        "@types/node": "^14.14.16",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "@types/react-redux": "^7.1.14",
        "@types/react-router-dom": "^5.1.6",
        "prettier": "^2.2.1",
        "react-app-rewired": "^2.1.8"
    }
}
